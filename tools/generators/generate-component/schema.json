{
  "$schema": "http://json-schema.org/schema",
  "cli": "nx",
  "$id": "generate-component",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "Component name",
      "x-prompt": "What should the component be called?",
      "$default": {
        "$source": "argv",
        "index": 0
      }
    },
    "library": {
      "type": "string",
      "description": "Project name",
      "x-prompt": "What project should the component be added to?"
    }
  },
  "required": [
    "name",
    "project"
  ]
}
