{
    "projectType": "library",
    "sourceRoot": "libs/text-input/src",
    "prefix": "bio",
    "targets": {
        "build": {
            "executor": "@nrwl/angular:package",
            "outputs": ["dist/libs/text-input"],
            "options": {
                "project": "libs/text-input/ng-package.json"
            },
            "configurations": {
                "production": {
                    "tsConfig": "libs/text-input/tsconfig.lib.prod.json"
                },
                "development": {
                    "tsConfig": "libs/text-input/tsconfig.lib.json"
                }
            },
            "defaultConfiguration": "production"
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": ["coverage/libs/text-input"],
            "options": {
                "jestConfig": "libs/text-input/jest.config.ts",
                "passWithNoTests": true
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "options": {
                "lintFilePatterns": ["libs/text-input/**/*.ts", "libs/text-input/**/*.html"]
            }
        },
        "storybook": {
            "executor": "@nrwl/storybook:storybook",
            "options": {
                "uiFramework": "@storybook/angular",
                "port": 4400,
                "config": {
                    "configFolder": "libs/text-input/.storybook"
                },
                "projectBuildConfig": "text-input:build-storybook"
            },
            "configurations": {
                "ci": {
                    "quiet": true
                }
            }
        },
        "build-storybook": {
            "executor": "@nrwl/storybook:build",
            "outputs": ["{options.outputPath}"],
            "options": {
                "uiFramework": "@storybook/angular",
                "outputPath": "dist/storybook/text-input",
                "config": {
                    "configFolder": "libs/text-input/.storybook"
                },
                "projectBuildConfig": "text-input:build-storybook"
            },
            "configurations": {
                "ci": {
                    "quiet": true
                }
            }
        }
    },
    "tags": []
}
