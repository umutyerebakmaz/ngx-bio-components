{
    "projectType": "library",
    "sourceRoot": "libs/badge/src",
    "prefix": "bio",
    "targets": {
        "build": {
            "executor": "@nrwl/angular:package",
            "outputs": ["dist/libs/badge"],
            "options": {
                "project": "libs/badge/ng-package.json",
                "tailwindConfig": "libs/badge/tailwind.config.js"
            },
            "configurations": {
                "production": {
                    "tsConfig": "libs/badge/tsconfig.lib.prod.json"
                },
                "development": {
                    "tsConfig": "libs/badge/tsconfig.lib.json"
                }
            },
            "defaultConfiguration": "production"
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": ["coverage/libs/badge"],
            "options": {
                "jestConfig": "libs/badge/jest.config.ts",
                "passWithNoTests": true
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "options": {
                "lintFilePatterns": ["libs/badge/**/*.ts", "libs/badge/**/*.html"]
            }
        },
        "storybook": {
            "executor": "@storybook/angular:start-storybook",
            "options": {
                "port": 4400,
                "configDir": "libs/badge/.storybook",
                "browserTarget": "badge:build-storybook",
                "compodoc": false
            },
            "configurations": {
                "ci": {
                    "quiet": true
                }
            }
        },
        "build-storybook": {
            "executor": "@storybook/angular:build-storybook",
            "outputs": ["{options.outputDir}"],
            "options": {
                "outputDir": "dist/storybook/badge",
                "configDir": "libs/badge/.storybook",
                "browserTarget": "badge:build-storybook",
                "compodoc": false
            },
            "configurations": {
                "ci": {
                    "quiet": true
                }
            }
        }
    },
    "tags": []
}
